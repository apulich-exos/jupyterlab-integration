{
  "name": "databrickslabs-jupyterlab-statusbar",
  "version": "2.0.0",
  "description": "A JupyterLab extension to show starting status of Databricks clusters.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Bernhard Walter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0",
    "@jupyterlab/apputils": "^2.0",
    "@jupyterlab/docregistry": "^2.0",
    "@jupyterlab/notebook": "^2.0",
    "@jupyterlab/statusbar": "^2.0",
    "@jupyterlab/coreutils": "^4.0",
    "@jupyterlab/services": "^5.0",
    "@lumino/polling": "^1.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typescript": "~3.7.5",
    "tslint": "^6.1.1",
    "prettier": "1.9.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "directories": {
    "lib": "lib"
  }
}
